<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body>
<h1><%= title %></h1>
<p>Welcome to photos</p>
<div id="photos">
        <% photos.forEach(function(photo){ %>

            <div class="photo">
                <img src='/photos/<%= photo.path %>'>
            </div>
            <h2><%= photo.name %></h2>
            <a href="/photo/<%= photo.id %>/download">download</a>
           <% }) %>
        %>
</div>
</body>
</html>
